{
  "name": "MyBlog",
  "packageManager": "yarn@3.3.0",
  "scripts": {
    "start": "node ./dist/index.js",
    "start:dev": "ts-node --files ./src/index.ts",
    "start:watch": "nodemon",
    "test": "mocha --require ts-node/register src/**/*.spec.ts --exit",
    "build": "rm -rf dist && tsc --sourceMap",
    "lint": "eslint . --ext ts",
    "fix": "eslint . --ext ts --fix"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "4.7.1",
    "fastify": "^4.10.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.10",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "chai": "^4.3.7",
    "eslint": "^8.29.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "prisma": "4.7.1",
    "sinon": "^15.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
